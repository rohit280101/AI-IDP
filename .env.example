# PostgreSQL Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-secure-password-here
POSTGRES_DB=ai_idp

# Application Configuration
DATABASE_URL=postgresql://postgres:your-secure-password-here@db:5432/ai_idp
REDIS_URL=redis://redis:6379/0

# JWT Configuration
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60

# Rate Limiting Configuration
UPLOAD_RATE_LIMIT=10
UPLOAD_RATE_WINDOW=60
SEARCH_RATE_LIMIT=30
SEARCH_RATE_WINDOW=60

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=AI Document Processing Platform

# Frontend Configuration
VITE_API_BASE_URL=http://localhost:8000/api/v1
# For production deployment, set this to your domain
# VITE_API_BASE_URL=https://api.yourdomain.com/api/v1

# Optional: OCR Configuration
OCR_ENABLED=true

# Optional: Embedding Service
EMBEDDING_MODEL=all-MiniLM-L6-v2
